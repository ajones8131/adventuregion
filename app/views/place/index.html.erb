<!DOCTYPE html>
<html>
<body>

  <h1>Current Places</h1>

  <a href="/place/new"> Add new place </a><br>
  <a href="/"> Return Home </a>

  <table id="place_table">
    <thead>
      <tr>
        <th>Place</th>
        <th>Place ID</th> 
        <th>Type</th>
        <th>Price</th>
        <th>Popularity</th> 
      </tr>
    </thead>
    <tbody>
      <% @places.each do |p| %>
          <tr>
            <td>'<%= p.name %>'</td>
            <td><%= p.placeid %></td>
            <td><%= p.type %></td>
            <td><%= p.price %></td>
            <td><%= p.popularity %></td>
          </tr>
      <% end %>
  </tbody>
  </table> 
</body>
</html>
